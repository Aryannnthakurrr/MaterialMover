{
  "name": "material-mover",
  "version": "1.0.0",
  "description": "Material Mover - buyer/seller/admin example with JWT auth and search webhook integration",
  "main": "services/backend/api/index.js",
  "scripts": {
    "start": "node services/backend/api/index.js",
    "dev": "concurrently \"nodemon services/backend/api/index.js\" \"cd services/search && python3 -m uvicorn app.main:app --host 0.0.0.0 --port 8000\"",
    "dev:backend": "nodemon services/backend/api/index.js",
    "dev:search": "cd services/search && python3 -m uvicorn app.main:app --host 0.0.0.0 --port 8000",
    "seed": "node seed/seed.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^5.1.2",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "google-auth-library": "^10.5.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mapbox-gl": "^3.18.1",
    "mongoose": "^7.8.7",
    "multer": "^2.0.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-map-gl": "^8.1.0",
    "serverless-http": "^3.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^2.0.22"
  }
}
